    <inherits name="com.vaadin.addon.charts.Widgetset"/>

    <inherits name="com.vaadin.addon.timeline.gwt.TimelineWidgetSet"/>

    <inherits name="org.vaadin.mockapp.samples.SampleWidgetset"/>

    
</module>
